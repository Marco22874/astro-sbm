{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "astro",
    "allowImportingTsExtensions": true,
    "noResolve": false,
    "moduleResolution": "bundler"
  }
}