{
  "redirects": [
    {
      "source": "/",
      "destination": "/it/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|html|json|xml|txt))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
